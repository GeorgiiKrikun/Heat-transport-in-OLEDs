doi = {10.1063/1.4913826},
file = {:home/georgii/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Estreicher, Gibbons, Bebek - 2015 - Thermal phonons and defects in semiconductors The physical reason why defects reduce heat flow, and.pdf:pdf},
issn = {0021-8979},
journal = {Journal of Applied Physics},
keywords = {ab initio calculations,heat transfer,molecular dynamics method,phonon-defect interactions,umklapp process},
month = {mar},
number = {11},
pages = {112801},
publisher = {AIP Publishing LLC},
title = {{Thermal phonons and defects in semiconductors: The physical reason why defects reduce heat flow, and how to control it}},
url = {http://aip.scitation.org/doi/10.1063/1.4913826},
volume = {117},
year = {2015}
}
